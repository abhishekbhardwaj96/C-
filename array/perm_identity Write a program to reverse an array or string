#include <string>
using namespace std;
int main() {
	//code
	int test;
	cin>>test;
	for(int i=0;i<test;i++){
	string str;
	char temp;
	cin>>str;
	int length = str.length();
	for(int i=0;i<length/2;i++){
	    temp = str[i];
	    str[i] = str[length-i-1];
	    str[length-1-i] = temp;
	}
	cout<<str<<"\n";
	
	}

    return 0;
}
